{
  "redirects": [
    {
      "source": "/admin/:path*",
      "destination": "/auth/admin/:path*",
      "permanent": false
    },
    {
      "source": "/realms/:path*",
      "destination": "/auth/realms/:path*",
      "permanent": false
    },
    {
      "source": "/resources/:path*",
      "destination": "/auth/resources/:path*",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://3.15.32.160/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "http://3.15.32.160/auth/:path*"
    }
  ]
}
